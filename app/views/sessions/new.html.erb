<%= content_tag :h1, "Log in" %>
<% if flash[:notice] %>
  <%= content_tag :p, flash[:notice] %>
<% end %>
<%= form_with url: login_path do |f| %>
  <%= f.text_field :email %><br>
  <%= f.password_field :password %><br>
  <%= link_to "Sign up", new_user_path %>
  <%= f.submit "Sign in!" %>
<% end %>